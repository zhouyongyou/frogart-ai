# 後端環境變數範例

NODE_ENV=development
PORT=4000

# 資料庫 MongoDB URI
MONGODB_URI=mongodb://localhost:27017/frogart_ai

# Ethereum RPC 與私鑰
ETH_RPC_URL=https://goerli.infura.io/v3/YOUR_INFURA_PROJECT_ID
PRIVATE_KEY=YOUR_DEPLOYER_PRIVATE_KEY

# 合約地址（部署後填上真實地址）
AIART_NFT_ADDRESS=0xYourAIArtNFTAddress
INVITER_TRACKER_ADDRESS=0xYourInviterTrackerAddress
REWARD_SPLITTER_ADDRESS=0xYourRewardSplitterAddress

# Pinata / Web3.storage API KEY & SECRET
PINATA_API_KEY=YOUR_PINATA_KEY
PINATA_API_SECRET=YOUR_PINATA_SECRET
